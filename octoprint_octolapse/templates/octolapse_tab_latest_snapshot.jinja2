<script type="text/html" id="latest-snapshot-template">
  <div id="octolapse_latest_snapshot_dialog" class="latest-snapshot-modal fade hide" >
          <a href="#" class="close" data-dismiss="modal" aria-hidden="true">&times;</a>
    <div id="snapshot_container" data-bind="foreach: latestSnapshotImageUrls">
      <img data-bind="attr:{src: imageUrl},css: Octolapse.Status.getAnimatedSnapshotClass($index())" ></img>
    </div>
    <a href="#" class="cancel btn btn-primary" data-bind="click: cancelLatestSnapshotDialog">Close</a>
  </div>
</script>
